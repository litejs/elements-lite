{
  "name": "elements-lite",
  "version": "0.6.0",
  "stability": 1,
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "license": "MIT",
  "homepage": "https://www.litejs.com/elements-lite/",
  "description": "DOM buildel for browser",
  "keywords": [
    "browser",
    "elements",
    "DOM",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "bundle-tests": "browserify tests/run-browser.js > tests/bundle.js",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": "git://github.com/litejs/elements-lite.git",
  "bugs": {
    "url": "https://github.com/litejs/elements-lite/issues"
  },
  "devDependencies": {
    "testman": "*",
    "buildman": "*",
    "browser-upgrade-lite": "*",
    "liquid-filters-lite": "*",
    "functional-lite": "*",
    "dom-lite": "*"
  },
  "buildman": {
    "dist/index-min.js": "index.js",
    "dist/index-modern-min.js": {
      "input": "index.js",
      "toggle": "modernBrowser"
    }
  }
}
